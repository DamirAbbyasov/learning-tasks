n = int(input())
prime_number = 0

for _ in range(n):
    num = int(input())
    is_prime = True
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            is_prime = False
            break
    if num < 2:
        is_prime = False
    if is_prime:
        prime_number += 1

print(prime_number)
