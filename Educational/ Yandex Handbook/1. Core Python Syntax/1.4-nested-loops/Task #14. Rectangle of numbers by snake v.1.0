height = int(input())
width = int(input())

max_number = height * width
digits = len(str(max_number))

for h in range(height):
    for w in range(width):
        if h % 2 == 0:
            n = h * width + w + 1
        else:
            n = h * width + (width - w - 1) + 1
        print(f"{n:>{digits}} ", end="")
    print()
